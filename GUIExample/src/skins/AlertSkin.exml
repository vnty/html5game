<?xml version="1.0" encoding="utf-8"?>
<e:Skin xmlns:e="http://ns.egret-labs.org/egret" xmlns:w="http://ns.egret-labs.org/wing"
	minHeight="230" minWidth="470" maxWidth="710">
	<w:HostComponent name="egret.AlertSkin" />
	<e:UIAsset width="100%" height="100%" source="alert-background" />
	<e:Label id="titleDisplay" fontFamily="Tahoma" size="36"
		textColor="0x727070" maxDisplayedLines="1" left="5" right="5" top="12"
		minHeight="28" verticalAlign="middle" textAlign="center" />
	<e:Label id="contentDisplay" fontFamily="Tahoma" size="22"
		textColor="0x727070" left="1" right="1" top="36" bottom="45" padding="10"
		verticalAlign="middle" textAlign="center" />
	<e:Group bottom="10" horizontalCenter="0">
		<e:layout>
			<e:HorizontalLayout horizontalAlign="center" gap="10"
				paddingLeft="20" paddingRight="20" />
		</e:layout>

		<e:Button id="firstButton" width="115" height="50" label="确定" />

		<e:Button id="secondButton" width="115" height="50" label="取消" />
	</e:Group>
</e:Skin>